<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <scene>
      <shadows>1</shadows>
    </scene>

    <gui>
      <camera name="user_camera">
        <pose>0 -2 1.75  0 0.35 1.57</pose>
